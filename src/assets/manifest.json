{
    "id": "org.elastos.trinity.dapp.friends",
    "version": "1.0.5",
    "version_code": 6,
    "name": "Contacts",
    "short_name": "Contacts",
    "short_description": "Connect with friends using their DID!",
    "description": "Manage Contacts by their DID and view their profile and apps they are using.",
    "start_url": "index.html",
    "type": "app",
    "start_visible": "hide",
    "category": "communication",
    "icons": [
        {
            "src": "assets/images/logo.png",
            "sizes": "512x512",
            "type": "image/png"
        },
        {
            "src": "assets/images/logo.png",
            "sizes": "128x128",
            "type": "image/png"
        }
    ],
    "author": {
        "name": "Elastos Foundation",
        "email": "contact@elastos.org",
        "website": "https://www.elastos.org"
    },
    "default_locale": "en",
    "plugins": [
        "Device",
        "NetworkStatus",
        "DIDPlugin",
        "Clipboard"
    ],
    "urls": [
        "http://*",
        "https://*"
    ],
    "background_color": "#4e8ef7",
    "theme_display": "show",
    "theme_color": "#4e8ef7",
    "intent_filters": [
        {
            "action": "handlescannedcontent_did"
        },
        {
            "action": "addfriend"
        },
        {
          "action": "pickfriend"
        }
    ]
}
